{
  "license": "../../LICENSE.header",
  "ignore": [".eslintcache", "translate-babel-config.babel", "**/*.license"],
  "licenseFormats": {
    "js|ts|css|scss": {
      "prepend": "/*",
      "append": " */",
      "eachLine": {
        "prepend": " * "
      }
    },
    "^Dockerfile|^.gitignore|^.prettierignore|^.eslintignore": {
      "eachLine": {
        "prepend": "# "
      }
    }
  },
  "trailingWhitespace": "TRIM",
  "regexIdentifier": "___"
}
