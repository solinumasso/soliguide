{
  "license": "../../LICENSE.header",
  "ignore": [".env*", ".eslintcache", ".prettierrc", "**/*.license"],
  "licenseFormats": {
    "^Dockerfile|^.gitignore|^.prettierignore": {
      "eachLine": {
        "prepend": "# "
      }
    },
    "html|md": {
      "prepend": "<!--",
      "append": "-->"
    },
    "js|ts|css|scss": {
      "prepend": "/*",
      "append": " */",
      "eachLine": {
        "prepend": " * "
      }
    }
  },
  "trailingWhitespace": "TRIM",
  "regexIdentifier": "___"
}
