{
  "license": "../../LICENSE.header",
  "ignore": [
    ".env*",
    ".eslintcache",
    "**/*.log",
    "**/*.txt",
    "**/*.xlsx",
    "**/*.docx",
    "**/*.geojson",
    "uploads/",
    "public/",
    "**/*.license"
  ],
  "licenseFormats": {
    "xml|svg": {
      "prepend": "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<!--",
      "append": "-->"
    },
    "html|md": {
      "prepend": "<!--",
      "append": "-->"
    },
    "js|ts|css|scss|mongodb": {
      "prepend": "/*",
      "append": " */",
      "eachLine": {
        "prepend": " * "
      }
    },
    "^Dockerfile|^.gitignore|^.prettierignore": {
      "eachLine": {
        "prepend": "# "
      }
    }
  },
  "trailingWhitespace": "TRIM",
  "regexIdentifier": "___"
}
