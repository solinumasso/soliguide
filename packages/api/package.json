{
  "name": "@soliguide/api",
  "version": "4.125.1",
  "description": "Soliguide API, all the services, initiatives and resources for people who encounter a difficulty",
  "author": "Solinum",
  "type": "commonjs",
  "license": "AGPL-3.0-only",
  "scripts": {
    "build": "yarn prebuild && tsc -p tsconfig.build.json && yarn postbuild",
    "categories:analyze": "tsx src/search/scripts/sync-categories.ts analyze",
    "categories:sync": "tsx src/search/scripts/sync-categories.ts sync",
    "categories:clean": "tsx src/search/scripts/sync-categories.ts clean",
    "categories:cleanup": "tsx src/search/scripts/sync-categories.ts cleanup",
    "format:fix": "prettier --cache -w .",
    "format": "prettier --cache -c .",
    "lint:fix": "eslint --cache ./ --fix",
    "lint": "eslint --cache ./",
    "migrate-create": "sh -c 'ts-node --transpile-only node_modules/migrate-mongo/bin/migrate-mongo create $0 -f migrate-mongo-config.ts'",
    "build-and-migrate-down": "yarn build && cp .env dist/.env && yarn migrate-down:prod && yarn migrate-status:prod",
    "migrate-down": "ts-node --transpile-only node_modules/migrate-mongo/bin/migrate-mongo down -f migrate-mongo-config.ts && yarn migrate-status",
    "migrate-down:prod": "cd dist && node ../node_modules/migrate-mongo/bin/migrate-mongo down -f migrate-mongo-config.js && cd ..",
    "migrate-status": "ts-node --transpile-only node_modules/migrate-mongo/bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate-status:prod": "cd dist && node ../node_modules/migrate-mongo/bin/migrate-mongo status -f migrate-mongo-config.js && cd ..",
    "build-and-migrate-up": "yarn build && cp .env dist/.env && yarn migrate-up:prod",
    "migrate-up": "ts-node --transpile-only node_modules/migrate-mongo/bin/migrate-mongo up -f migrate-mongo-config.ts",
    "migrate-up:prod": "cd dist && node ../node_modules/migrate-mongo/bin/migrate-mongo up -f migrate-mongo-config.js && cd ..",
    "watch": "cp -r ../common/dist/cjs/translations/locales ./resources && nodemon --config nodemon.json src/app.ts | pino-pretty",
    "postbuild": "cp -r ../common/dist/cjs/translations/locales ./resources && cp -r ./resources ./dist/resources",
    "prebuild": "rimraf dist",
    "start": "node --trace-warnings dist/src/app.js",
    "test": "ENV=test MONGODB_URI='mongodb://127.0.0.1:27017/soliguide_test?replicaSet=rs0' TZ=Europe/Paris jest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@aws-sdk/client-s3": "^3.787.0",
    "@google-cloud/translate": "^8.3.0",
    "@sentry/node": "^9.47.1",
    "@sentry/profiling-node": "^9.47.1",
    "@soliguide/common": "workspace:*",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.7",
    "apicache": "^1.6.3",
    "axios": "^1.13.5",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.4",
    "cache-manager": "^6.4.3",
    "class-validator": "^0.14.2",
    "commander": "^14.0.3",
    "compression": "1.8.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.6",
    "cron": "^4.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "delay": "5.0.0",
    "docxtemplater": "^3.50.0",
    "docxtemplater-image-module-free": "^1.1.1",
    "dot-object": "^2.1.5",
    "dotenv": "16.6.1",
    "env-schema": "^6.0.1",
    "express": "4.21.2",
    "express-validator": "^7.2.1",
    "fastest-levenshtein": "^1.0.16",
    "fuse.js": "^7.1.0",
    "google-libphonenumber": "^3.2.40",
    "html-entities": "^2.6.0",
    "i18next": "^22.5.1",
    "i18next-fs-backend": "^2.3.1",
    "json2csv": "5.0.7",
    "jsonwebtoken": "^9.0.2",
    "libreoffice-convert": "^1.5.1",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "migrate-mongo": "^12.1.3",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.3",
    "multer": "1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "pino-http": "^10.5.0",
    "pizzip": "^3.1.7",
    "posthog-node": "^4.12.0",
    "slug": "^9.1.0",
    "striptags": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "4.6.3",
    "validator": "^13.15.26",
    "xlsx": "^0.18.5",
    "xmlbuilder": "15.1.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/apicache": "^1.6.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/cron": "^2.4.3",
    "@types/dot-object": "^2.1.6",
    "@types/express": "^4.17.25",
    "@types/fs-extra": "^11.0.4",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/jest": "^29.5.14",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/multer": "^1.4.13",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.19.33",
    "@types/pizzip": "^3.0.5",
    "@types/safe-timers": "^1.1.2",
    "@types/slug": "^5.0.9",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "13.11.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-promise": "^7.1.0",
    "franc-min": "^6.2.0",
    "jest": "29.7.0",
    "license-check-and-add": "^4.0.5",
    "lint-staged": "^15.5.1",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.3",
    "prettier": "^2.8.8",
    "rimraf": "^6.1.2",
    "supertest": "^7.1.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">= 22"
  },
  "lint-staged": {
    "*": "prettier --cache --ignore-unknown -w",
    "*.{ts,js}": "eslint --cache --fix"
  }
}
