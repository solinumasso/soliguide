{
  "license": "../../LICENSE.header",
  "ignore": [".angular", ".eslintcache", "**/*.license"],
  "licenseFormats": {
    "xml|svg": {
      "prepend": "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<!--",
      "append": "-->"
    },
    "html|md": {
      "prepend": "<!--",
      "append": "-->"
    },
    "js|ts|css|scss": {
      "prepend": "/*",
      "append": " */",
      "eachLine": {
        "prepend": " * "
      }
    },
    "^Dockerfile|^.gitignore|^.prettierignore|^.editorconfig": {
      "eachLine": {
        "prepend": "# "
      }
    }
  },
  "trailingWhitespace": "TRIM",
  "regexIdentifier": "___"
}
